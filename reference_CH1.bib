@article{Poletaev2008,
author = {Poletaev, Maxim and Robinson, Chris},
title = {Human Capital Specificity: Evidence from the Dictionary of Occupational Titles and Displaced Worker Surveys, 1984–2000},
journal = {Journal of Labor Economics},
volume = {26},
